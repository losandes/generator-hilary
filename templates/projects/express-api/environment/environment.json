{
    "port": 3000,
    "storeThisConfigInMemory": true,
    "cors": {
        "mode": "off",
        "originWhiteList": {
            "null": true,
            "http://localhost:3000": true
        },
        "allowCredentials": true,
        "allowMethods": ["GET", "POST", "PATCH", "PUT", "DELETE", "OPTIONS", "HEAD"],
        "allowHeaders": ["Authorization", "Accepts", "Content-Type", "If-Match", "If-Modified-Since", "If-None-Match", "If-Unmodified-Since", "Range", "X-Requested-With"],
        "exposeHeaders": ["Content-Length", "Date", "ETag", "Expires", "Last-Modified", "X-Powered-By"],
        "cacheDuration": "120",
        "denialMessage": { "status": 403, "message": "CORS Error: your origin, {origin}, is not whitelisted." }
    },
    "docs": {
        "files": [
            "./docs/index.md",
            "./docs/_versioning.md",
            "./docs/_legos.md",
            "./docs/_status-codes.md",
            "./docs/_about.md"
        ],
        "languages": [
            { "name": "All", "highlightClass": "lang-all" },
            { "name": "Shell", "highlightClass": "lang-bash" },
            { "name": "JavaScript", "highlightClass": "lang-js" }
        ]
    },
    "versions": {
        "beta": [
            { "name": "20150828-beta", "comparable": 20150828, "number": "v2b2", "responseHeader": "application/vnd.<%= projectName %>.20150828-beta+json" },
            { "name": "20150825-beta", "comparable": 20150825, "number": "v2b1", "responseHeader": "application/vnd.<%= projectName %>.20150825-beta+json" }
        ],
        "alpha": [
            { "name": "20150828-alpha", "comparable": 20150828, "number": "v2a2", "responseHeader": "application/vnd.<%= projectName %>.20150828-alpha+json" },
            { "name": "20150825-alpha", "comparable": 20150825, "number": "v2a1", "responseHeader": "application/vnd.<%= projectName %>.20150825-alpha+json" }
        ],
        "rc": [
            { "name": "20150828-rc", "comparable": 20150828, "number": "v2rc2", "responseHeader": "application/vnd.<%= projectName %>.20150828-rc+json" },
            { "name": "20150825-rc", "comparable": 20150825, "number": "v2rc1", "responseHeader": "application/vnd.<%= projectName %>.20150825-rc+json" }
        ],
        "prod": [
            { "name": "20150820", "comparable": 20150820, "number": "v2", "responseHeader": "application/vnd.<%= projectName %>.20150820+json" },
            { "name": "20150807", "comparable": 20150807, "number": "v1", "responseHeader": "application/vnd.<%= projectName %>.20150807+json" }
        ],
        "vendorPrefix": "vnd.<%= projectName %>",
        "responseHeader": "X-<%= projectName %>-Media-Type"
    }
}
